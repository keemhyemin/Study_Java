package pack02_Static;

public class Ex02_CaculatorMain {
	public static void main(String[] args) {
		
		System.out.println("plus: " + Ex02_Caculator.plus(10, 10));
		System.out.println("minius: " + Ex02_Caculator.minus(20, 10));
		System.out.println("mul: " + Ex02_Caculator.mul(5, 5));
		System.out.println("div: " + Ex02_Caculator.div(50, 10));
		
		// 외부에서 접근하는 것이기 때문에 구분하기 위해 클래스 이름을 적어준다
	}
}
