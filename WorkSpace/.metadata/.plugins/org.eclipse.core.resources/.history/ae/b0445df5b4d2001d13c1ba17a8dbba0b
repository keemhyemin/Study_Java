package pack01_If;

public class Ex04_Ifif {
	public static void main(String[] args) {
		// 블럭킹 기본 개념: '{' <- 메모리 올림 / '}' <- 메모리 내림
		// 자바 프로그램은 메모리를 효율적으로 쓰기위해서 메모리에 올리고 내리는 과정 존재
		// 블럭킹(지역)은 계속해서 중첩이 가능하다.
		// 제어문이 3개 이상 중첩이 되는 경우 내가 맞게 짜고 있는가 의심해보자
		int score = 10;
		//if문 중첩시켜 score 짝수이면 짝수 출력
		// score가 10이면 10이라는 문자열 출력
		// 11보다 작으면 11보다 작음이라는 문자열을 출력하게 코딩해보자
		if ( score % 2 == 0) {
			System.out.println("짝수");
		if (score == 10) {
			System.out.println("10");
		if (score > 11) {
			System.out.println("11보다 작음");
				}
			}
		}
				
	}
}
