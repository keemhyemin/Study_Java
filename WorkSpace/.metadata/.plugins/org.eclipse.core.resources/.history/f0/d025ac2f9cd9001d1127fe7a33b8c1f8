package pack02_tryCatch;

public class Ex02_TryCatchFinally {
	public static void main(String[] args) {
		// 요리: 가스 불 ON, 요리 시작, 요리 끝, 가스 불 OFF, 먹기(종료)
		try {
			System.out.println("가스불 ON");
			System.out.println("요리 시작");
			int errorInt = 10/0; // 요리하다가 갑자기 무슨 일 생김(예외)
			System.out.println("요리끝");
			System.out.println("가스불 OFF");
			System.out.println("종료");
		}catch (Exception e) {
			
		}
	}

}
