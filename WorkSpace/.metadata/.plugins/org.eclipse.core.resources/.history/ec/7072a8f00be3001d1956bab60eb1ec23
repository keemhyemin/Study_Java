package pack02.extend;

public class SuperClass {
	// super();
	// 1세대: 두개의 수를 입력 받아 합을 return하는 메소드 sum 구현해보기
	// 차를 return하는 메소드 minus 구현해보기

	public int sum(int x, int y) {

		return x + y;
	}

	public int minus(int x, int y) {

		return x - y;
	}

	public static void main(String[] args) {
		SuperClass ss = new SuperClass();
		System.out.println("sum: " + ss.sum(10, 3));
		System.out.println("minus: " + ss.minus(10, 3));
	}
}
