package pack01.list;

public class Ex02_Vector {
	public static void main(String[] args) {
		// 사용빈도가 없거나 매우 낮기 때문에 이론만 살피고 넘어감.
		// List라는 Interface를 상속받았기 때문에 기본 구조는 ArrayList, LinkedList, Vector 전부 비슷함.
		// ? TestDTO라는 클래스를 묶어서 저장하고 싶다.
		// List 생성해보기
		// List에 요소 추가해보기
		// List에 있는 요소 출력해보기
		TestDTO dto = new TestDTO("성", "이름");
		List<TestDTO> list = new Vector<TestDTO>();
	}
}
