
public class JapangiDTO {
	private String item;
	private int price, cnt;
	int money;
	String id, pw;
	
	public JapangiDTO(String item, int price, int cnt) {
		
		this.item = item;
		this.price = price;
		this.cnt = cnt;
	}

	public String getItem() {
		return item;
	}

	public void setItem(String item) {
		this.item = item;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}

	public int getCnt() {
		return cnt;
	}

	public void setCnt(int cnt) {
		this.cnt = cnt;
	}
	
	
	
	

}
