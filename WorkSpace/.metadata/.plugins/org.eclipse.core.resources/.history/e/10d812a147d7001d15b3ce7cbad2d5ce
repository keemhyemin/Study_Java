package pack02_Array;

public class Ex02_Array {
	public static void main(String[] args) {
		// 1~10의 크기를 가진 int 배열 만들고 10 ~ 100까지 값 할당하고 출력
		
		int[] numArray = new int[10];
		numArray[0] = 10;
		numArray[1] = 20;
		numArray[2] = 30;
		numArray[3] = 40;
		numArray[4] = 50;
		numArray[5] = 60;
		numArray[7] = 70;
		numArray[8] = 80;
		numArray[9] = 90;
		numArray[10] = 100;
		for (int i=0; i<10; i++) {
			System.out.println("numArray[" + i + "]: " + numArray[i]);
		}
	}

}
