package test01_Variable;

import java.util.Scanner;

public class Test02 {
	public static void main(String[] args) {
//		소수 입력받아 정수형으로 출력하기
		Scanner sc = new Scanner(System.in);
		String str = sc.nextLine();
		double data = Double.parseDouble(str);
		int iNum = (int)data;
		System.out.println(iNum);
	}
}
