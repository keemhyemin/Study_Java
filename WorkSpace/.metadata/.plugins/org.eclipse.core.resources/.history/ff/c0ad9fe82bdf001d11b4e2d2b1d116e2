
public class JapangiDAO {
	 void display(JapangiDTO dto) {
		 System.out.println(dto.getNum() + ". " + dto.getItem() + " " + "금액: " + dto.getPrice() + " " + "수량: " + dto.getCnt());
	 }
	 
	 public void display(JapangiDTO[] dtoArr) {
		 for (int i=0; i<dtoArr.length; i++) {
			 System.out.println(dtoArr[i]);
		 }
	 }

}
