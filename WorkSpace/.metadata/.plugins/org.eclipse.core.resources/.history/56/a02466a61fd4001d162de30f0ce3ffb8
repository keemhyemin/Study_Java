package pack04_While;

public class Test01_While {
	public static void main(String[] args) {

		// 가로 출력
		int i = 1;
		while (i < 9) {
			int j = 1;
			i++;
			while (j <= 9) {
				System.out.print(i + "x" + j + "=" + (i * j) + " ");
				j++;
			}
			System.out.println();
		}
		System.out.println("==========================================");
		// 세로 출력
		int k = 1;
		while (k < 9) {
			int l = 1;
			k++;
			while (l <= 9) {
				System.out.println(k + "x" + l + "=" + (k * l) + " ");
				l++;
			}
		}
		
	}

}

