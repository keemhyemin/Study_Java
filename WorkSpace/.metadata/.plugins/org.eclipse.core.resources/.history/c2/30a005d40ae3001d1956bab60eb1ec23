package pack02.extend;

public class SuperClass {
	// super();
	// 1세대: 두개의 수를 입력 받아 합을 return하는 메소드 sum 구현해보기
	//								차를 return하는 메소드 minus 구현해보기
	
	int x;
	int y;
	
	public int method1(int x, int y) {
		int sum = x + y;
		return sum;
	}
	public int method2(int x, int y) {
		int minus = x - y;
		return minus;
	}
	public static void main(String[] args) {
		SuperClass ss = new SuperClass();
		System.out.println("sum: " + ss.method1(10, 3));
		System.out.println("sum: " + ss.method2(10, 3));
	}
}
