package pack07_Escape;

public class Ex01_Escape {
	public static void main(String[] args) {
		//문자열 사이에 어떠한 기호를 넣어서 띄어쓰기나 줄바꿈, 특수문자 표현 등을 하기 위한 문자
		System.out.println("이름 : \" 김혜민 \" ");
		System.out.println("번호\t이름\t직업"); // tab
		System.out.print("문자1\n"); //ln == line new == 새 줄에 쓰기
		System.out.print("문자2");
	}

}
