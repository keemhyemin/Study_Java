
public class Ex01_Operator {
	public static void main(String[] args) {
		int x = -100;
		int result1 = +x;
		int result2 = -x;
		System.out.println("(+x) result1 = " + result1);  // 결과는 -100이 나올 것 같다
		System.out.println("(+x) x*1 = " + x * 1);
		System.out.println("(-x) result2 = " + result2);  // 결과는 +100이 나올 것 같다
//		- 는 부호를 반전시키는 규칙이 있다.( + 는 ㄴㄴ)
	} 

}
