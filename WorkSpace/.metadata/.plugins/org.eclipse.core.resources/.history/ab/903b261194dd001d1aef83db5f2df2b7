package pack02_Static;

public class Ex02_CaculatorMain {
	public static void main(String[] args) {
		
		System.out.println(Ex02_Caculator.plus(10, 10));
		System.out.println(Ex02_Caculator.minus(20, 10));
		System.out.println(Ex02_Caculator.mul(5, 5));
		System.out.println(Ex02_Caculator.div(50, 10));
	}
}
