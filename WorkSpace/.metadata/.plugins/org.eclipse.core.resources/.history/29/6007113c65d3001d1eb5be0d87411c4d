package pack03_For;

import java.util.Scanner;

public class Test01_For {
	public static void main(String[] args) {

		for (int i = 5; i >= 1; i--) {
			for (int j = i; j <= 5; j++) {
				System.out.print("★");
			}
			System.out.println();
		}
		Scanner sc = new Scanner(System.in);
		int num = Integer.parseInt(sc.nextLine());
		if (num < 1) {
			System.out.println("잘못된 숫자");
		}

	}
}
